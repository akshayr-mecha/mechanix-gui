title: Status Bar # Empty
app:
  id: mechanix.shell.status-bar # Status bar
  text_multithreading: true # false
  antialiasing: true # false
  try_opengles_first: true # false
window:
  size: 480 # 480
  height: 34 # 37
  width: 480
  layer_shell:
    margin:
      top: 0
      right: 0
      bottom: 0
      left: 0
    anchor:
      top: true
      right: true
      bottom: false
      left: true
  position: [0, 0] # [0, 0]
  min_size: [480, 40] # None
  max_size: null # None
  visible: true # true
  resizable: true # true
  decorations: true # true
  transparent: true # false
  always_on_top: false # false
  icon_path: null # Todo
fonts:
css:
  default: /usr/share/mechanix/shell/status-bar/assets/css/style.css
layout:
  left: ["clock"]
  center: ["window_title"]
  right: ["wireless", "bluetooth", "battery"]
modules:
  clock:
    format: "%I:%M %p" # https://docs.rs/chrono/latest/chrono/format/strftime/index.html
  bluetooth:
    icon:
      on: /usr/share/mechanix/shell/status-bar/assets/icons/bluetooth/bluetooth_on_icon.svg
      off: /usr/share/mechanix/shell/status-bar/assets/icons/bluetooth/bluetooth_off_icon.svg
      connected: /usr/share/mechanix/shell/status-bar/assets/icons/bluetooth/bluetooth_connected_icon.svg
      not_found: /usr/share/mechanix/shell/status-bar/assets/icons/bluetooth/bluetooth_not_found_icon.svg
  wireless:
    icon:
      off: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_off_icon.svg
      on: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_on_icon.svg
      low: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_low_icon.svg
      weak: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_weak_icon.svg
      good: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_good_icon.svg
      strong: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_strong_icon.svg
      not_found: /usr/share/mechanix/shell/status-bar/assets/icons/wireless/wireless_not_found_icon.svg
  battery:
    icon:
      level_100: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_100_icon.svg
      level_90: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_90_icon.svg
      level_80: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_80_icon.svg
      level_70: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_70_icon.svg
      level_60: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_60_icon.svg
      level_50: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_50_icon.svg
      level_40: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_40_icon.svg
      level_30: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_30_icon.svg
      level_20: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_20_icon.svg
      level_10: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_10_icon.svg
      level_0: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_0_icon.svg
      not_found: /usr/share/mechanix/shell/status-bar/assets/icons/battery/battery_not_found_icon.svg
    charging_icon:
      level_100: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_100_icon.svg
      level_90: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_90_icon.svg
      level_80: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_80_icon.svg
      level_70: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_70_icon.svg
      level_60: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_60_icon.svg
      level_50: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_50_icon.svg
      level_40: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_40_icon.svg
      level_30: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_30_icon.svg
      level_20: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_20_icon.svg
      level_10: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_10_icon.svg
      level_0: /usr/share/mechanix/shell/status-bar/assets/icons/battery/charging/battery_0_icon.svg
