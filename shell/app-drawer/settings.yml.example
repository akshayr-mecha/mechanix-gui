title: App Drawer # Empty
app:
  id: mechanix.shell.app-drawer # app-drawer
  text_multithreading: true # false
  antialiasing: true # false
  try_opengles_first: true # false
window:
  size: [480, 440] # [1024, 768]
  position: [0, 0] # [0, 0]
  min_size: [480, 40] # None
  max_size: null # None
  visible: true # true
  resizable: true # true
  decorations: true # true
  transparent: true # false
  always_on_top: false # false
  icon_path: null # Todo
layout:
  left: []
  center: []
  right: []
modules:
  search:
    icon:
      default: /usr/share/mechanix/shell/app-drawer/assets/icons/search_icon.svg
  home:
    icon:
      default: /usr/share/mechanix/shell/app-drawer/assets/icons/home_icon.svg
  back:
    icon:
      default: /usr/share/mechanix/shell/app-drawer/assets/icons/back_icon.svg
  clear:
    icon:
      default: /usr/share/mechanix/shell/app-drawer/assets/icons/clear_icon.svg
  not_found:
    icon:
      default: /usr/share/mechanix/shell/app-drawer/assets/icons/not_found_icon.png
      small: /usr/share/mechanix/shell/app-drawer/assets/icons/not_found_small_icon.png
